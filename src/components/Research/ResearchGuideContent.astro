<script>
  document.addEventListener("DOMContentLoaded", () => {
    const catchupToggleFunction = () => {
      const content = document.getElementById("catchupToggleContent")
      const icon = document.getElementById("catchupToggle")
      if (content && icon) {
        if (content.classList.contains("hidden")) {
          content.classList.remove("hidden")
          content.setAttribute("aria-hidden", "false")
          icon.classList.remove("fa-angle-down")
          icon.classList.add("fa-angle-up")
        } else {
          content.classList.add("hidden")
          content.setAttribute("aria-hidden", "true")
          icon.classList.remove("fa-angle-up")
          icon.classList.add("fa-angle-down")
        }
      }
    }

    document
      ?.getElementById("catchupButton")
      ?.addEventListener("click", catchupToggleFunction)
  })
</script>

<style>
  .catchup-button {
    font-size: 1rem;
    background-color: transparent;
    color: #ffc107;
    border: 1px solid #ffc107;
    border-radius: 0.375rem;
    padding: 0.375rem 0.75rem;
    transition:
      color 0.15s ease-in-out,
      background-color 0.15s ease-in-out,
      border-color 0.15s ease-in-out;
  }

  .catchup-button:hover {
    background-color: #ffc107;
    color: black;
  }
</style>

